{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "node": "12" } }
    ],
  ],
  "sourceMaps": "inline",
  "retainLines": true,
  plugins: [
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-async-to-generator",
      {
        "module": "bluebird",
        "method": "coroutine"
      }
    ]
  ]
}

